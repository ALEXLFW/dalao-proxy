{
    "cacheDirname": ".dalao-cache",
    "watch": true,
    "host": "localhost",
    "port": "1111",
    "target": "http://localhost:8080",
    "static": "http://localhost:8080",
    "rewrite": true,
    "cache": true,
    "cacheMaxAge": ["s", 10],
    "responseFilter": [
        "code",
        200
    ],
    "info": false,
    "headers": {
        "Access-Control-Expose-Headers": "X-My-Custom-Header, X-Another-Custom-Header",
        "Access-Control-Max-Age": "3600"
    },
    "emptyRoutes": false,
    "proxyTable": {
        "/customer": {
            "target": "http://139.196.213.165:9132/"
        },
        "/image": {
            "target": "http://139.196.213.165:9132/api"
        },
        "/pic": {
            "target": "http://139.196.213.165:9132/api"
        }
    }
}