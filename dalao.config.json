{
    "watch": true,
    "host": "localhost",
    "port": "9088",
    "target": "localhost:9088",
    "rewrite": false,
    "cache": false,
    "info": false,
    "headers": {
        "Via": "HTTP/1.1 dalao-proxy"
    },
    "emptyRoutes": false,
    "proxyTable": {
        "/": {
            "path": "/"
        },
        "/jd": {
            "target": "https://www.jd.com",
            "path": "/",
            "rewrite": true
        }
    }
}